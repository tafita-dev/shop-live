{
  "expo": {
    "name": "Live",
    "slug": "Live",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "expoapps",
    "owner": "tafita",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "navigationBar": {
      "visible": "true",
      "backgroundColor": "#0f172a",
      "barStyle": "light-content"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tafita.Live",
      "config": {
        "facebookAppId": "804429838843232",
        "facebookClientToken": "b8c730698cd6ab316e74dc66ebf6f89a",
        "facebookDisplayName": "MYSTORE"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tafita.Live",
      "config": {
        "facebookAppId": "804429838843232",
        "facebookClientToken": "b8c730698cd6ab316e74dc66ebf6f89a",
        "facebookDisplayName": "MYSTORE"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "react-native-fbsdk-next",
        {
          "appID": "804429838843232",
          "clientToken": "b8c730698cd6ab316e74dc66ebf6f89a",
          "displayName": "MYSTORE"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7f9088a9-ddbe-487c-848e-85bfb7cb08a1"
      }
    }
  }
}
