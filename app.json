{
  "expo": {
    "name": "live",
    "slug": "Live",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "expoapps",
    "owner": "tafita",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tafita.live"
    },

    "android": {
      "package": "com.tafita.live",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "VIBRATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "RECEIVE_BOOT_COMPLETED",
        "NOTIFICATIONS",
        "POST_NOTIFICATIONS"
      ],
      "softwareKeyboardLayoutMode": "resize",
      "jsEngine": "hermes",
      "googleServicesFile": "./google-services.json"
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      "expo-font",
      "expo-notifications",
      "expo-splash-screen",
      [
        "expo-image-picker",
        {
          "photosPermission": "L’application a besoin d’accéder aux photos pour sélectionner des images."
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "7f9088a9-ddbe-487c-848e-85bfb7cb08a1"
      }
    }
  }
}
